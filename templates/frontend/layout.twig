			{% include('frontend/nav.twig') %}
		</nav>
		{# Carousel only homePage #}
		{% block carousel %}{% endblock %}
		<main>
			{% block content %}{% endblock %}
		</main>
		{% block footer %}
			{% include('frontend/footer.twig') %}
		{% endblock %}
	</body>
</html>
