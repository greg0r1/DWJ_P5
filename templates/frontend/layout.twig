			{% include('frontend/head.twig') %}
			<title>
				{% block title %}
					{{title}}
				{% endblock %}
			</title>
		{% endblock %}
	</head>

	<body>
		<header>
			{% include('frontend/header.twig') %}
		</header>
		{% include('frontend/popupLogin.twig') %}
